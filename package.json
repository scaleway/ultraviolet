{
  "name": "@scaleway/ui",
  "version": "0.35.0",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "yarn": ">=1.22.x"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/module.js",
  "jsnext:main": "dist/module.js",
  "scripts": {
    "build": "rollup -c",
    "build:profile": "cross-env PROFILE=true yarn run build",
    "build:storybook": "build-storybook --docs",
    "check:deps": "npx depcheck . --skip-missing=true --ignores='bin,eslint,webpack,jest,husky,@semantic-release/*,@commitlint/*,@babel/*,babel-*'",
    "commit": "npx git-cz -a",
    "dev": "yarn run start:storybook",
    "start:storybook": "start-storybook -p 6006 --docs",
    "format": "prettier --write '**/*.{js,json,md,mdx}'",
    "lint:fix": "yarn run lint --fix",
    "lint": "eslint --cache .",
    "prebuild": "shx rm -rf dist",
    "test:coverage": "yarn run test --coverage",
    "test:watch": "yarn run test --watch",
    "test": "jest",
    "transfer": "yarn build && cp -f ./dist/* ../shire/node_modules/@scaleway/ui/dist"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/__stories__/**/*.{js,mdx}"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": ".reports",
          "outputName": "tests.xml"
        }
      ]
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.jest/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@smooth-ui/core-em": "^10.0.0",
    "emotion-theming": "^10.0.27",
    "react": "^16.13.0 || ^17.0.1",
    "react-dom": "^16.13.0 || ^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@emotion/babel-preset-css-prop": "^10.2.1",
    "@emotion/core": "^10.1.1",
    "@emotion/eslint-plugin": "^11.0.0",
    "@emotion/styled": "^10.0.27",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-url": "^5.0.1",
    "@scaleway/eslint-config-react": "^1.1.10",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.6",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@smooth-ui/core-em": "^10.1.0",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@storybook/theming": "^6.1.10",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/react": "^11.2.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "emotion-theming": "^10.0.27",
    "eslint": "^7.15.0",
    "eslint-formatter-gitlab": "^2.0.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-emotion": "^10.0.32",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "read-pkg": "^5.2.0",
    "rollup": "^2.34.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-postcss": "^3.1.8",
    "semantic-release": "^17.3.0",
    "shx": "^0.3.3",
    "zxcvbn": "^4.4.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@scaleway/random-name": "^1.1.3",
    "polished": "^4.0.5",
    "prop-types": "^15.7.2",
    "react-countup": "^4.3.3",
    "react-flatten-children": "^1.1.2",
    "react-markdown": "^5.0.3",
    "react-select": "^3.1.1",
    "react-toastify": "^6.1.0",
    "reakit": "^1.3.1"
  },
  "resolutions": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
