{
  "name": "@scaleway/ui",
  "version": "0.80.0",
  "description": "Scaleway UI",
  "homepage": "https://github.com/scaleway/scaleway-ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaleway/scaleway-ui"
  },
  "keywords": [
    "react",
    "reactjs",
    "ui"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "yarn": ">=1.22.x"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "sideEffects": false,
  "main": "dist/module.js",
  "module": "dist/module.js",
  "jsnext:main": "dist/module.js",
  "scripts": {
    "build": "rollup -c",
    "build:profile": "cross-env PROFILE=true yarn run build",
    "build:storybook": "build-storybook",
    "check:deps": "npx depcheck . --skip-missing=true --ignores='bin,eslint,webpack,jest,husky,@semantic-release/*,@commitlint/*,@babel/*,babel-*'",
    "commit": "npx git-cz -a",
    "start": "STORYBOOK_ENVIRONMENT=development start-storybook -p 6006",
    "start:production": "STORYBOOK_ENVIRONMENT=production start-storybook -p 6006",
    "format": "prettier --write '**/*.{js,json,md,mdx}'",
    "lint:fix": "yarn run lint --fix",
    "lint": "eslint --cache --ext js,ts,mdx .",
    "prebuild": "shx rm -rf dist",
    "test:coverage": "yarn run test --coverage",
    "test:watch": "yarn run test --watch",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix --ext js,ts,md,mdx"
    ],
    "*.mdx": [
      "prettier --write",
      "eslint --fix --ext mdx"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/__stories__/**/*.{js,mdx}"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": ".reports",
          "outputName": "tests.xml"
        }
      ]
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@scaleway)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.jest/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ]
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "react": "^16.13.0 || ^17.0.1",
    "react-dom": "^16.13.0 || ^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@emotion/cache": "^11.1.3",
    "@emotion/eslint-plugin": "^11.0.0",
    "@emotion/jest": "^11.1.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@scaleway/eslint-config-react": "^1.5.0",
    "@scaleway/use-i18n": "^2.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/addon-a11y": "^6.2.5",
    "@storybook/addon-actions": "^6.2.5",
    "@storybook/addon-docs": "^6.2.5",
    "@storybook/addon-essentials": "^6.2.5",
    "@storybook/addon-links": "^6.2.5",
    "@storybook/addons": "^6.2.5",
    "@storybook/builder-webpack5": "^6.2.5",
    "@storybook/react": "^6.2.5",
    "@storybook/theming": "^6.2.5",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^13.0.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.21.1",
    "eslint": "^7.19.0",
    "eslint-mdx": "^1.9.1",
    "eslint-plugin-mdx": "^1.8.2",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "read-pkg": "^6.0.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.38.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-visualizer": "^5.3.0",
    "semantic-release": "^17.3.7",
    "shx": "^0.3.3",
    "timekeeper": "^2.2.0",
    "zxcvbn": "^4.4.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@scaleway/random-name": "^1.2.0",
    "@xstyled/emotion": "^2.2.2",
    "intl-tel-input": "^17.0.12",
    "polished": "^4.1.0",
    "prop-types": "^15.7.2",
    "react-circular-progressbar": "^2.0.4",
    "react-countup": "^4.3.3",
    "react-datepicker": "^3.6.0",
    "react-flatten-children": "^1.1.2",
    "react-markdown": "^5.0.3",
    "react-select": "^4.0.2",
    "react-toastify": "^7.0.3",
    "react-use-clipboard": "^1.0.7",
    "reakit": "^1.3.5"
  },
  "contributors": [
