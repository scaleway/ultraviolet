{
  "name": "@scaleway/ui",
  "version": "0.19.0",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=v14.14.0",
    "yarn": ">=1.22.4"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/module.js",
  "jsnext:main": "dist/module.js",
  "scripts": {
    "build": "rollup -c",
    "build:profile": "cross-env PROFILE=true yarn run build",
    "build:storybook": "build-storybook --docs",
    "check:deps": "npx depcheck . --skip-missing=true --ignores='bin,eslint,webpack,jest,husky,@semantic-release/*,@commitlint/*,@babel/*,babel-*'",
    "commit": "npx git-cz -a",
    "dev": "yarn run start:storybook",
    "start:storybook": "start-storybook -p 6006 --docs",
    "format": "prettier --write '**/*.{js,json,md,mdx}'",
    "lint:fix": "yarn run lint --fix",
    "lint": "eslint --cache .",
    "prebuild": "shx rm -rf dist",
    "test:coverage": "yarn run test --coverage",
    "test:watch": "yarn run test --watch",
    "test": "jest",
    "transfer": "yarn build && cp -f ./dist/* ../shire/node_modules/@scaleway/ui/dist"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@smooth-ui/core-em": "^10.0.0",
    "emotion-theming": "^10.0.27",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.5",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@smooth-ui/core-em": "^10.1.0",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addons": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@storybook/theming": "^6.0.27",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "cross-env": "^7.0.2",
    "emotion-theming": "^10.0.27",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "read-pkg": "^5.2.0",
    "rollup": "^2.32.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-postcss": "^3.1.8",
    "semantic-release": "^17.2.1",
    "shx": "^0.3.2",
    "zxcvbn": "^4.4.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "docker-names": "^1.1.1",
    "polished": "^3.6.7",
    "prop-types": "^15.7.2",
    "react-countup": "^4.3.3",
    "react-flatten-children": "^1.1.2",
    "react-select": "^3.1.0",
    "react-toastify": "^6.0.9",
    "reakit": "^1.2.5"
  },
  "resolutions": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
